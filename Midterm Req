import java.util.Scanner;

public class Input1_DELUNA {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        // Input three strings
        System.out.println();
        String str1 = "HI";
        String str2 = "HELLO";
        String str3 = "WORLD";

        // Print out the three strings using only 1 print statement
        System.out.print(str1 + "\n" + str2 + "\n" + str3 + "\n");
    }
}